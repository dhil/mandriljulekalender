{
    // Environment options
    "browser": true,
    "devel": true,

    // Enforcing options
    "esversion": 11,
    "forin": true,
    "freeze": true,
    "strict": "global",
    "varstmt": true,
    "trailing": true,

    // Relaxing options
    "asi": false,
    "evil": false,
    "funcscope": false,
    "laxcomma": true,

    // Global variables whitelist
    "globals": {}
}
